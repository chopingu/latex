\ProvidesPackage{preamble/font} 

% Fonts 
\usepackage{ebgaramond-maths}
\usepackage[T1]{fontenc}
\usepackage[UKenglish]{babel}

% Larger space between lines
\linespread{1.15}\selectfont

% No indentation
\setlength\parindent{0pt}

\DeclareMathSymbol{+}{\mathbin}{textoperators}{`+}
\DeclareMathSymbol{-}{\mathbin}{textoperators}{`-}
\DeclareMathSymbol{=}{\mathrel}{textoperators}{`=}
\DeclareMathDelimiter{(}{\mathopen} {textoperators}{"28}{largesymbols}{"00}
\DeclareMathDelimiter{)}{\mathclose}{textoperators}{"29}{largesymbols}{"01}
\DeclareMathDelimiter{[}{\mathopen} {textoperators}{"5B}{largesymbols}{"02}
\DeclareMathDelimiter{]}{\mathclose}{textoperators}{"5D}{largesymbols}{"03}
